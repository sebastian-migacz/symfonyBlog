            <h2><a href="{{ path('post_show',{'id':post.id}) }}">{{ post.title }}</a></h2>

            <p>{{ post.lead }}</p>

        </div>
    {% endfor %}

    {{ knp_pagination_render(posts) }}
{% endblock %}
